Image IN ("*\\tree.com", "*\\WMIC.exe", "*\\doskey.exe", "*\\sc.exe") OR OriginalFileName IN ("wmic.exe", "DOSKEY.EXE", "sc.exe") ParentCommandLine IN ("* > %TEMP%\\*", "* > %TMP%\\*")